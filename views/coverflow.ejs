    <script>
        var $el = $('.coverflow').coverflow({
            active : 3,
            visibleAside: 3
        });

        $(window).resize(debounce( function() {
            $el.coverflow();
        }, 20, true ));
    </script>

    <script>
        var current = $('#client4');
        
        $(".client").click(function(event) {

            //check if the element's attribute is set to true
            //meaning the client is the one showing on the coverflow
            var isSelected = $(this).attr('data-selected')

            if (isSelected == "false") {
                event.preventDefault();
                current.attr('data-selected', 'false');
                current = $(this);
                $(this).attr('data-selected', 'true');
            }
        });
        
    </script>    
